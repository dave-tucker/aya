{
    "categories": [
      {
        "title": "## 🚀 Features",
        "labels": ["feature"],
        "exhaustive": true
      },
      {
        "title": "## 🐛 Fixes",
        "labels": ["fix"],
        "exhaustive": true
      },
      {
        "title": "## 🧪 Tests",
        "labels": ["test"],
        "exhaustive": true
      }
    ],
    "ignore_labels": [
      "ignore"
    ],
    "tag_resolver": {
      "filter": {
        "pattern": "<from-workflow>"
      },
      "transformer": {
        "pattern": "<from-workflow>",
        "target": "$1"
      }
    }
}
